node {
 def app
   stage ('clone rep')
    {
     git "https://github.com/awspriya/dockerfiles.git"
     }
   stage ('build image')
    {
     app=docker.build("priyaawscloud/testubuntu")
      }
    stage ('push image')
     {
      docker.withRegistry('https://hub.docker.com','dockertest1')
        {
        app.push("${env.BUILD_NUMBER}")
         }
     }
     }
